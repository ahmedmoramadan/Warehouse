@model MemberViewModel

@{
    ViewData["Title"] = "Add";
}

<div class="container-fluid d-flex tender-parent  mt-10">
    <div class="text-center tender">
        <h1 class="tender-header">Add Member </h1>
        <hr />
        <div class="row">
            <div class="col-md-12">
                <form asp-action="Add">
                    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                    <div class="form-group">
                        <label asp-for="Name" class="control-label error"></label>
                        <input asp-for="Name" class="form-control" />
                        <span asp-validation-for="Name" class="text-danger error"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="PhoneNumber" class="control-label error"></label>
                        <input asp-for="PhoneNumber" class="form-control" />
                        <span asp-validation-for="PhoneNumber" class="text-danger error"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="Password" class="control-label error"></label>
                        <input asp-for="Password" class="form-control" />
                        <span asp-validation-for="Password" class="text-danger error"></span>
                    </div>
                    <div class="form-group d-flex justify-content-end space">
                        <input type="submit" value="Add" class="btn btn-primary" />
                    </div>
                   
                </form>
            </div>
        </div>
    </div>
</div>
@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}
